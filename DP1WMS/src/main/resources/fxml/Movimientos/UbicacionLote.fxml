            <TableView fx:id="tablaUbicaciones" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="c_indice" prefWidth="48.0" text="N°" />
                <TableColumn fx:id="c_cantidad" prefWidth="84.0" text="Cantidad" />
