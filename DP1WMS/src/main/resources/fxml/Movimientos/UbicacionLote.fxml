              </columns>
            </TableView>
         </children>
      </StackPane>
      <Button layoutX="284.0" layoutY="338.0" mnemonicParsing="false" onAction="#cancelarOperacion" prefHeight="25.0" prefWidth="94.0" text="Regresar" />
      <Button layoutX="195.0" layoutY="88.0" mnemonicParsing="false" onAction="#modificarUbicacion" prefHeight="25.0" text="Modificar Ubicación" />
      <Label layoutX="32.0" layoutY="30.0" text="Ubicación del Lote ">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="agregarUbicacion" layoutX="32.0" layoutY="88.0" mnemonicParsing="false" onAction="#agregarUbicacion" prefHeight="25.0" text="Agregar Ubicación" />
   </children>
</AnchorPane>
