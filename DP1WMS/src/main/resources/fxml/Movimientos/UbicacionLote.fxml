<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dp1wms.controller.Movimientos.UbicacionLoteController">
   <children>
      <StackPane layoutX="32.0" layoutY="146.0" prefHeight="177.0" prefWidth="539.0">
