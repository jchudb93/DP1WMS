<AnchorPane prefHeight="634.0" prefWidth="683.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dp1wms.controller.Tabu.AlmacenRutaController">
   <children>
      <ScrollPane fitToHeight="true" fitToWidth="true" layoutX="17.0" layoutY="217.0" prefHeight="348.0" prefWidth="643.0">
         <content>
            <AnchorPane prefHeight="346.0" prefWidth="530.0">
               <children>
                  <GridPane fx:id="almacen_layout" gridLinesVisible="true" layoutX="0.5" layoutY="0.5">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="20" prefWidth="20.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="20" prefHeight="20.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                  </GridPane>
               </children>
            </AnchorPane>
         </content>
      </ScrollPane>
      <HBox layoutX="113.0" layoutY="163.0" spacing="30.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button fx:id="generar_ruta" mnemonicParsing="false" onAction="#click_generar_ruta" text="Generar Ruta" />
            <Button fx:id="guardar_ruta" mnemonicParsing="false" text="Guardar Ruta" />
            <Button mnemonicParsing="false" onAction="#limpiarGrilla" text="Limpiar" />
            <Button mnemonicParsing="false" text="Ver historial" />
         </children>
         <padding>
            <Insets left="30.0" right="30.0" />
         </padding>
      </HBox>
      <VBox layoutX="93.0" layoutY="86.0" spacing="20.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <GridPane hgap="10.0" prefHeight="65.0" prefWidth="200.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="311.0" minWidth="10.0" prefWidth="294.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="355.0" minWidth="10.0" prefWidth="339.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <HBox alignment="TOP_RIGHT" spacing="10.0">
                     <children>
                        <Label text="Num Iteraciones" />
                        <TextField fx:id="numeroIteraciones" prefHeight="25.0" prefWidth="160.0" HBox.hgrow="NEVER" />
                     </children>
                  </HBox>
                  <HBox alignment="TOP_RIGHT" prefHeight="29.0" prefWidth="233.0" spacing="10.0" GridPane.columnIndex="1">
                     <children>
                        <Label prefHeight="13.0" text="Máx. Iteraciones sin Mejora" HBox.hgrow="ALWAYS" />
                        <TextField fx:id="numIteracionesSinMejora" alignment="CENTER_RIGHT" prefHeight="26.0" prefWidth="160.0" HBox.hgrow="NEVER" />
                     </children>
                  </HBox>
                  <HBox alignment="TOP_RIGHT" spacing="10.0" GridPane.rowIndex="1">
                     <children>
                        <Label text="Tamaño Lista Tabu" />
                        <TextField fx:id="listaTabuTamanho" prefHeight="26.0" prefWidth="160.0" HBox.hgrow="NEVER" />
                     </children>
                  </HBox>
                  <HBox alignment="TOP_RIGHT" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                     <children>
                        <Label text="Tiempo Maximo (segundos)" />
                        <TextField fx:id="tiempoMaximoTF" alignment="CENTER_RIGHT" prefHeight="25.0" prefWidth="160.0" HBox.hgrow="NEVER" />
                     </children>
                  </HBox>
               </children>
               <opaqueInsets>
                  <Insets bottom="20.0" />
               </opaqueInsets>
            </GridPane>
         </children>
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
      <Label layoutX="153.0" layoutY="40.0" text="Eleccion de Ruta de Despacho">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
